{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT a.id as id, a.file as file, a.path as path, a.version as unpack_version, f.hash as hash, f.size as size\n        FROM assets a\n        INNER JOIN\n            files f ON a.file = f.id\n        WHERE\n            a.version = $1\n            AND ($1::varchar IS NULL OR a.path LIKE $2)\n            AND ($2::varchar IS NULL OR f.hash = $3)\n        ",
