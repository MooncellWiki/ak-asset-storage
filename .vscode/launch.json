  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "start",
      "cargo": {
        "args": [
          "build",
          "--bin=ak_asset_storage",
          "--package=ak_asset_storage"
        ],
        "filter": {
          "name": "ak_asset_storage",
          "kind": "bin"
        }
      },
      "args": ["start"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "seed",
      "cargo": {
        "args": [
          "build",
          "--bin=ak_asset_storage",
          "--package=ak_asset_storage"
        ],
        "filter": {
          "name": "ak_asset_storage",
          "kind": "bin"
        }
      },
      "args": ["seed", "--csv-path", "./test.csv"],
      "cwd": "${workspaceFolder}"
    }
  ]
}
